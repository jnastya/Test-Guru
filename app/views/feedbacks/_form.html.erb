<h1><%= t('.coment') %></h1>
<%= form_with(model: @feedback, local: true) do |form| %>
  <%= render 'shared/errors', resource: @feedback %>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email, id: :feedback_email %>
  </p>

  <p>
    <%= form.label :message %><br>
    <%= form.text_area :message, id: :feedback_message %>
  </p>

  <p><%= form.submit %></p>
<% end %>
